        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.balibabu.noteapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "5cef3a3b-041b-4f18-b4be-173e0473ff74"
      }
    },
    "owner": "balibabu"
  }
